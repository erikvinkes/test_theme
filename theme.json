    "color": "@TextColor",
    "fontSize": "@TextSize",
    "backgroundColor": "@BackgroundColor",
    "dataColors": {
		"primaryColor": "#0b9ce2",
		"othersColor": "#CC9B00",
		"errorColor": "#CC4000",
		"nullColor": "#CCCCCC"
